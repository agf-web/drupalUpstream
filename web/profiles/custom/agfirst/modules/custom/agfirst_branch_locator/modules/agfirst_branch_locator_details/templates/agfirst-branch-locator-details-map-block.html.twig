{% if detail_data.address|trim or detail_data.city|trim or detail_data.state|trim or detail_data.zip|trim %}
  <iframe width="100%" height="400px" title="{{ detail_data.branch }} Map" frameborder="0" style="border:0" src="https://maps.google.com/maps?hl=en&amp;q={{ detail_data.address }}+{{ detail_data.city }}+{{ detail_data.state }}+{{ detail_data.zip }}+United+States&amp;t=m&amp;z=14&amp;output=embed"></iframe>
{% endif %}
